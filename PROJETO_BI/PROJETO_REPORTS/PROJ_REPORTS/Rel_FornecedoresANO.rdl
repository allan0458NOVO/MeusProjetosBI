  <DataSets>
    <DataSet Name="SET_FORNECEDORES">
      <Query>
        <DataSourceName>DS_FORNECEDORES</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ANO">
            <Value>=Parameters!ANO.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        FORNECEDOR, ANO, QUANTIDADE, TOTAL_VENDIDO
FROM            V_ANALISE_FORNECEDOR
WHERE ANO in (@ANO)
ORDER BY FORNECEDOR</CommandText>
      </Query>
      <Fields>
        <Field Name="FORNECEDOR">
          <DataField>FORNECEDOR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ANO">
          <DataField>ANO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QUANTIDADE">
          <DataField>QUANTIDADE</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TOTAL_VENDIDO">
          <DataField>TOTAL_VENDIDO</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SET_ANO">
      <Query>
        <DataSourceName>DS_FORNECEDORES</DataSourceName>
        <CommandText>SELECT DISTINCT DIM_TEMPO.ANO
FROM         DIM_TEMPO INNER JOIN
                         FATO ON DIM_TEMPO.IDSK = FATO.IDTEMPO</CommandText>
      </Query>
      <Fields>
        <Field Name="ANO">
          <DataField>ANO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
